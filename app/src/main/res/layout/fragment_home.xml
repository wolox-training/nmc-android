<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center"
    android:orientation="vertical"
    android:id="@+id/vFragmentHome">

    <ar.com.wolox.android.example.ui.customView.CustomToolbar
        android:id="@+id/vToolbarFragmentHome"
        android:layout_width="match_parent"
        android:layout_height="@dimen/spacing_largest_extra"/>

    <com.google.android.material.tabs.TabLayout
        android:id="@+id/vTabLayoutFragmentHome"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />


    <androidx.viewpager.widget.ViewPager
        android:id="@+id/vViewPagerFragmentHome"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />


</LinearLayout>